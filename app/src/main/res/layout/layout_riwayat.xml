<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/listtemposq"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:clickable="true"
    android:orientation="vertical"
    android:layout_margin="8dp"
    android:focusable="true"
    android:background="@color/colorWhite"
    >

    <LinearLayout
        android:background="@color/colorWhite"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_margin="8dp"
        >

        <!--<TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Tanggal"

            android:textColor="@color/inputborder"
            android:textStyle="bold" />-->

        <TextView
            android:id="@+id/tx_presensi"
            android:textColor="@color/grey_darken_4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="@string/presensi_dari"
            android:textSize="16sp"
            android:textStyle="bold"
            android:layout_marginBottom="8dp"
            />

        <TextView
            android:id="@+id/tgl"
            android:textColor="@color/grey_darken_4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="--------"
            android:textSize="16sp"
            android:textStyle="bold"
            android:layout_marginBottom="8dp"
            />

        <LinearLayout
            android:id="@+id/llRiwayatPresensi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"

            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">


                    <TextView
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="Presensi"

                        android:textColor="@color/inputborder"
                        android:textStyle="bold" />


                    <TextView
                        android:id="@+id/abs_masuk"
                        android:layout_weight="1"
                        android:textColor="@color/grey_darken_4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="--------"
                        android:textSize="15sp"
                        android:textStyle="bold"/>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="Pulang"

                        android:textColor="@color/inputborder"
                        android:textStyle="bold" />


                    <TextView
                        android:id="@+id/abs_pulang"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:textColor="@color/grey_darken_4"
                        android:text="-------"
                        android:textSize="15sp"
                        android:textStyle="bold"/>


                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">


                    <TextView
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="Istirahat"

                        android:textColor="@color/inputborder"
                        android:textStyle="bold" />


                    <TextView
                        android:id="@+id/abs_istirahat"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:textColor="@color/grey_darken_4"
                        android:text="--------"
                        android:textSize="15sp"
                        android:textStyle="bold"/>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="Selesai Istirahat"

                        android:textColor="@color/inputborder"
                        android:textStyle="bold" />


                    <TextView
                        android:id="@+id/abs_selesai_istirahat"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="--------"
                        android:textColor="@color/grey_darken_4"
                        android:textSize="15sp"
                        android:textStyle="bold"/>


                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">


                    <TextView
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="Mulai Lembur"

                        android:textColor="@color/inputborder"
                        android:textStyle="bold" />


                    <TextView
                        android:id="@+id/abs_lembur"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="--------"
                        android:textColor="@color/grey_darken_4"
                        android:textSize="15sp"
                        android:textStyle="bold"/>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="Selesai Lembur"

                        android:textColor="@color/inputborder"
                        android:textStyle="bold" />


                    <TextView
                        android:id="@+id/abs_selesai_lembur"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="--------"
                        android:textColor="@color/grey_darken_4"
                        android:textSize="15sp"
                        android:textStyle="bold"/>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/llRiwayatPresensiStatus"
            android:background="@color/colorWhite"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:orientation="vertical"
            android:layout_margin="8dp"
            android:gravity="center"
            >

            <TextView
                android:id="@+id/tvStatusPresensi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="DAY OFF"
                android:textStyle="bold"
                android:textSize="32sp"
                android:gravity="center"
                android:textColor="@color/transparentRed"
                />

        </LinearLayout>

    </LinearLayout>

    <View
        android:id="@+id/divider5"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:background="#B3DDFF" />

</LinearLayout>